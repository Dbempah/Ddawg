grade = int(input("Enter in your grade. "))
if(grade <= 100 and grade > 89):
    print('A')
elif(grade <= 89 and grade > 79):
    print('B')
elif(grade <= 79 and grade > 69):
    print('C')
elif(grade <= 69 and grade > 59):
    print('D')
   >>>>>>>>>>>>>>>>>>>>>
   
   def getGrade(n):
    if (grade <= 100 and grade > 89):
        return 'A'
    elif(grade <= 89 and grade > 69):
        return 'Between B and C'
    elif(grade <= 69 and grade > 59):
        return 'Between D and F'
grade = int(input("Enter in your grade."))
print ('Your grade is', getGrade ( grade ))
