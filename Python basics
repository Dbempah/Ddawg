import random
numList = []
for i in range (20):
    numList.append(random.randint(1,1000))
for i in numList:
    if(i%2==0):
        print(i, 'Is an even number')
    else:
        print(i, 'Is an odd number')
<<>>>>>>>>>>>>>>>>>>>>>>>>>
Odd,even

import random
numList = []
oddList = []
evenList = []
for i in range(20):
    numList.append(random.randint(1,1000))
for i in numList:
    if(i%2==0):
        evenList.append(i)
    else:
        oddList.append(i)
print(numList)
print(evenList)
print(oddList)
